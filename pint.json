{
    "preset": "laravel",
    "rules": {
        "no_trailing_whitespace_in_comment": false,
        "no_trailing_whitespace": false,
        "trailing_comma_in_multiline": false,
        "simplified_null_return": true,
        "single_quote": false,
        "braces": false,
        "Laravel/laravel_phpdoc_separation": false,
        "new_with_braces": {
            "anonymous_class": false,
            "named_class": false
        }
    }
}